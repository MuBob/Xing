<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.wjx.xing.activitys.InfoselfActivity">
    <RelativeLayout style="@style/title_layout">
        <TextView
            style="@style/title_center"
            android:text="个人信息"
            />
        <TextView
           style="@style/title_left"
           android:id="@+id/message_left"
            />
        <TextView
            android:id="@+id/message_right"
            style="@style/title_right"
            />
    </RelativeLayout>
    <ListView
        android:paddingTop="20dp"
        android:id="@+id/lv_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </ListView>
</LinearLayout>

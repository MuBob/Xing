<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.wjx.xing.activitys.normal.InfoSkillActivity">
    <include layout="@layout/layout_title"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rl_accounting"
            android:layout_width="match_parent"
            style="@style/textview_items"
            android:layout_height="wrap_content">
            <TextView
                android:text="会计"
                style="@style/textview_style"
                android:layout_alignParentLeft="true"
                />
            <TextView
                android:id="@+id/tv_accounting"
                style="@style/textview_style"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <TextView
            style="@style/View_line"/>
        <RelativeLayout
            android:id="@+id/rl_computer"
            android:layout_width="match_parent"
            style="@style/textview_items"
            android:layout_height="wrap_content">
            <TextView
                android:text="计算机"
                style="@style/textview_style"
                android:layout_alignParentLeft="true"
                />
            <TextView
                android:id="@+id/tv_computer"
                style="@style/textview_style"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <TextView
            style="@style/View_line"/>
        <RelativeLayout
            android:id="@+id/rl_civil_engineer"
            android:layout_width="match_parent"
            style="@style/textview_items"
            android:layout_height="wrap_content">
            <TextView
                android:text="注册土木工程师"
                style="@style/textview_style"
                android:layout_alignParentLeft="true"
                />
            <TextView
                android:id="@+id/tv_civil_engineer"
                style="@style/textview_style"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
        <TextView
            style="@style/View_line"/>
        <RelativeLayout
            android:id="@+id/rl_safety_engineer"
            android:layout_width="match_parent"
            style="@style/textview_items"
            android:layout_height="wrap_content">
            <TextView
                android:text="注册安全工程师"
                style="@style/textview_style"
                android:layout_alignParentLeft="true"
                />
            <TextView
                android:id="@+id/tv_safety_engineer"
                style="@style/textview_style"
                android:layout_alignParentRight="true"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>
